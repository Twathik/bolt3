{
  "name": "backend",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "dotenv -e .envs/dev.env -- ts-node-dev --no-notify --respawn --exit-child --transpile-only src/server",
    "start": "yarn deploy:prod && yarn seed:prod && dotenv -e .envs/prod.env -- node dist/index",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && tsc",
    "generate": "prisma generate",
    "migrate:dev": "dotenv -e .envs/dev.env -- npx prisma migrate dev",
    "deploy:dev": "dotenv -e .envs/dev.env -- npx prisma migrate deploy",
    "seed:dev": "dotenv -e .envs/dev.env -- npx prisma db seed",
    "reset:dev": "dotenv -e .envs/dev.env -- npx prisma migrate reset ",
    "migrate:prod": "dotenv -e .envs/prod.env -- npx prisma migrate dev ",
    "deploy:prod": "dotenv -e .envs/prod.env -- npx prisma migrate deploy",
    "seed:prod": "dotenv -e .envs/prod.env -- npx prisma db seed",
    "studio": "prisma studio -p 5333",
    "forceKill": "kill -9 $(lsof -t -i:4000)"
  },
  "prisma": {
    "schema": "prisma/schema.prisma",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  },
  "dependencies": {
    "@babel/runtime": "^7.23.1",
    "@prisma/client": "^5.4.0",
    "@types/express": "^4.17.18",
    "@types/graphql-fields": "^1.3.6",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "class-validator": "^0.14.0",
    "cookie": "^0.5.0",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dgraph-js": "^21.3.1",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.3.0",
    "express": "^4.18.2",
    "graphql": "^15.8.0",
    "graphql-fields": "^2.0.3",
    "graphql-http": "^1.22.0",
    "graphql-scalars": "^1.22.2",
    "node-geocoder": "^4.2.0",
    "prisma": "^5.4.0",
    "prisma-erd-generator": "^1.11.1",
    "prisma-zod-generator": "^0.8.13",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.6.2",
    "type-graphql": "^1.1.1",
    "typedi": "^0.10.0",
    "typegraphql-prisma": "0.27.0",
    "typesense": "^1.7.1",
    "uuid": "^9.0.1",
    "zod-prisma": "^0.5.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/body-parser": "^1.19.3",
    "@types/cookie": "^0.5.2",
    "@types/cors": "^2.8.14",
    "@types/node": "^20.8.2",
    "@types/node-geocoder": "^4.2.3",
    "@types/uuid": "^9.0.4",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.0.3",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.2.2"
  },
  "resolutions": {
    "graphql": "^15.8.0"
  }
}
